{
  "name": "data-contest-toolkit",
  "version": "0.0.1",
  "description": "A toolkit for conducting machine learning trials against confidential data",
  "license": "MIT",
  "scripts": {
    "test": "mocha",
    "pretest:functional": "npm run compile",
    "test:functional": "mocha --config test/functional/.mocharc.yml",
    "check": "gts check",
    "clean": "gts clean && rimraf ./dist",
    "compile": "tsc -p .",
    "dct": "node build/src/cli/dct.js",
    "dct:dev": "npm run compile && node -r dotenv/config build/src/cli/dct.js",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "laboratory": "node build/src/laboratory/server/main.js",
    "laboratory:dev": "npm run compile && node -r dotenv/config build/src/laboratory/server/main.js",
    "laboratory:package:appservice": "npm run compile && mkdir -p dist && cp -r --parents package*.json build/src dist && cd dist && npm install --ignore-scripts --production && zip -r $npm_package_name.zip package.json package-lock.json build/src node_modules",
    "worker": "node build/src/worker/app.js",
    "worker:dev": "npm run compile && node -r dotenv/config build/src/worker/app.js"
  },
  "dependencies": {
    "@azure/identity": "^1.0.2",
    "@azure/storage-queue": "^12.0.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "commander": "^5.0.0",
    "dockerode": "^3.1.0",
    "dotenv": "^8.2.0",
    "env-var": "^6.0.4",
    "express": "^4.17.1",
    "fp-ts": "^2.5.3",
    "io-ts": "^2.1.2",
    "js-yaml": "^3.13.1",
    "luxon": "^1.22.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^4.1.1",
    "strong-error-handler": "^3.4.0",
    "tedious": "^8.2.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/ajv": "^1.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-http": "^4.2.0",
    "@types/dockerode": "^2.5.26",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/js-yaml": "^3.12.3",
    "@types/luxon": "^1.22.0",
    "@types/mocha": "^7.0.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.12.31",
    "@types/request-promise": "^4.1.46",
    "@types/sinon": "^7.5.2",
    "@types/sinonjs__fake-timers": "^6.0.0",
    "@types/tedious": "^4.0.0",
    "@types/uuid": "^3.4.7",
    "@types/validator": "^12.0.1",
    "@types/yargs": "^15.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.0.2",
    "chai-http": "^4.3.0",
    "gts": "^1.1.2",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.0",
    "source-map-support": "^0.5.16",
    "typescript": "^3.8.3",
    "typescript-json-schema": "^0.42.0",
    "xmlhttprequest": "^1.8.0"
  }
}
